{
  "name": "@openclaw/node-red-tool",
  "version": "2026.2.6-0",
  "description": "OpenClaw Node-RED Flow control tools via Admin API",
  "type": "module",
  "dependencies": {
    "@sinclair/typebox": "0.34.48"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": ["./index.ts"],
    "install": {
      "npmSpec": "@openclaw/node-red-tool",
      "localPath": "extensions/node-red-tool",
      "defaultChoice": "local"
    }
  }
}
